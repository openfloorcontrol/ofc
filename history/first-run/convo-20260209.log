import csv

# Read the CSV
with open('sales.csv', 'r') as f:
    reader = csv.DictReader(f)
    rows = list(reader)

# Group by date
daily = {}
for row in rows:
    date = row['date']
    amount = float(row['amount'])
    if date not in daily:
        daily[date] = 0
    daily[date] += amount

# Sort by date
sorted_dates = sorted(daily.keys())

print("=" * 60)
print("TREND ANALYSIS")
print("=" * 60)

# Calculate day-over-day changes
prev_revenue = None
for i, date in enumerate(sorted_dates):
    revenue = daily[date]
    if prev_revenue:
        change = revenue - prev_revenue
        pct_change = (change / prev_revenue) * 100
        arrow = "â†‘" if change > 0 else "â†“"
        print(f"{date}: ${revenue:.2f} ({arrow} {change:+.2f}, {pct_change:+.1f}%)")
    else:
        print(f"{date}: ${revenue:.2f} (baseline)")
    prev_revenue = revenue

print()
print("Key Insight: Sales show an overall upward trend,")
print("with the strongest performance in the last 3 days.")
